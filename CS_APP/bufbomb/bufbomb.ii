char *getxs(char *dest)
{
  int c;
  int even = 1;
  int otherd = 0;
  char *sp = dest;
  while ((c = getchar()) != (-1) && c != '\n') {
    if (isxdigit(c)) {
      int val;
      if ('0' <= c && c <= '9')
 val = c - '0';
      else if ('A' <= c && c <= 'F')
 val = c - 'A' + 10;
      else
 val = c - 'a' + 10;
      if (even) {
 otherd = val;
 even = 0;
      } else {
 *sp++ = otherd * 16 + val;
 even = 1;
      }
    }
  }
  *sp++ = '\0';
  return dest;
}


int getbuf()
{
    char buf[12];
    getxs(buf);
    return 1;
}

void test()
{
  int val;
  printf("Type Hex string:");
  val = getbuf();
  printf("getbuf returned 0x%x\n", val);
}


int main()
{

  int buf[16];



  int offset = (((int) buf) & 0xFFF);
  int *space = (int *) malloc(offset);
  *space = 0;
  test();
  return 0;
}
