ll modular_exponentiation(ll n)
{
   ll result = 1;
   ll power = n;

   while(n)
   {
      if(n&1)
	  {
          result = (result*power)%modulo;
	  }
     	 
       power=(power*power)%modulo;
       n >>= 1;
   }

   return result;
}

