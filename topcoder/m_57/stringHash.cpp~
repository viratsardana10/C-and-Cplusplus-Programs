#include <iostream>
#include <cstring>
using namespace std;
long hash(string s);

int main()
{
	string str;
	cout<<"Enter String: ";
	cin>>str;

	//for()

	long hashValue=hash(str);
	cout<<str<<" : "<<hashValue<<endl;

	return 0;
}

long hash(string s)
{
	long hash_value= 5381;
	int length=s.size();

	for(int i=0;i<length;i++)
	{
		hash_value=(hash_value*33)^s[i];
		cout<<s[i]<<"  ";
		//cout<<hash_value<<"   ";
	}
        cout<<endl;
	return hash_value;
}

